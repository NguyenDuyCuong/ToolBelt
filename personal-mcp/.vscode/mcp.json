{
	"servers": {
		"serena": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant",
				"--project",
				"${workspaceFolder}"
			]
		},
		"everything": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-everything"
			]
		},
		"deepwiki": {
			"url": "https://mcp.deepwiki.com/sse"
		},
		"chrome-devtools": {
			"command": "npx",
			"args": [
				"-y",
				"chrome-devtools-mcp@latest"
			]
		},
		"MCP_DOCKER": {
			"command": "docker",
			"args": [
				"mcp",
				"gateway",
				"run"
			],
			"env": {
				"LOCALAPPDATA": "C:\\Users\\cuong\\AppData\\Local",
				"ProgramData": "C:\\ProgramData",
				"ProgramFiles": "C:\\Program Files"
			}
		},
		"personal-mcp":
		{
			"url": "http://127.0.0.1:8000/mcp",
			"type": "http",
		}
	},
	"inputs": []
}